using System;

class _5_TheBiggestOfThreeNumbers
{
    static void Main()
    {
        double a = double.Parse(Console.ReadLine());
        double b = double.Parse(Console.ReadLine());
        double c = double.Parse(Console.ReadLine());

        double maxOne = (Math.Max(a, b));
        double maxTwo = (Math.Max(b, c));

        Console.WriteLine(Math.Max(maxOne, maxTwo));
    }
}

